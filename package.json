{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11"
  },
  "scripts": {
    "dev": "@tailwindcss/cli -i ./src/assets/css/tailwind.css -o ./dev/output.css",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,html,js}": "prettier --write"
  }
}
